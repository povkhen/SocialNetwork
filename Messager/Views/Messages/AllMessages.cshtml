@model List<Message>

<div class="p-2 w-100 bg-dark">
    Dialog
</div>

<div class="p-2 row justify-content-start">
        @{
            foreach (TextMessage mes in Model)
            {
                @Html.Partial("TextMessage", mes)
                <div class="w-100"></div>
            }
        }
</div>

<div class="p-2 w-100 bg-dark">
    FieldForSend
</div>
